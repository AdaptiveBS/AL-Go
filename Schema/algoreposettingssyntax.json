{
    "$id": "AL-Go for GitHub Repo Settings File",
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "schema for AL-Go for GitHub Repo Settings File",
    "type": "object",
    "properties": {
        "type": {
            "enum": [
                "PTE",
                "AppSource"
            ],
            "enumDescriptions": [
                "Per Tenant Extension",
                "AppSource app"
            ],
            "description": "Repository type"
        },
        "templateUrl": {
            "type": "string",
            "description": "URL the GitHub repository template for this repository"
        }
    }
}